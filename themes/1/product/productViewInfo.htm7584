				<li class="value">
					<span class="price">{$_product['price']['price']}</span>
					<!--{if $_product['vat'] != ''}-->
					<span class="vat">$_product['vat']</span>
					<!--{/if}-->
				</li>
				<li class="clearfix"></li>
				<li class="key">{lang code_qr}</li>
				<li class="value"><img src="{$_product['qr']}"></li>
				<li class="clearfix"></li>
				<!--{if $_product['price_old'] != 0}-->
				<li class="key">{lang label_price}</li>
				<li class="value"><span class="price">{$_product['price']['price']}</span></li>
				<li class="clearfix"></li>
				<!--{/if}-->
				<!--{if $_product['percent'] != 0}-->
				<li class="key">{lang saving}</li>
				<li class="value">{$_product['price']['percent']}</li>
				<li class="clearfix"></li>
				<!--{/if}-->
				<!--{if !empty($_product['code'])}-->
				<li class="key">{lang label_product_model}</li>
				<li class="value">{$_product['code']}</li>
				<li class="clearfix"></li>
				<!--{/if}-->

				<!--{if !empty($_product['unit'])}-->
					<li class="key">{lang unit}</li>
					<li class="value">{$_product['unit']}</li>
					<li class="clearfix"></li>
				<!--{/if}-->
				
				<!--{if !empty($_product['color'])}-->
				<li class="key">{lang color} ssssssssssssssssssssss</li>
				<li class="value">
					<!--{loop $_product['color'] $k_color $v_color}-->
						<!--{if !empty($v_color['name'])}-->
							{$v_color['name']} <span style="width: 23px;height: 10px;border: black solid 1px;background: {$v_color['code']};">&nbsp;&nbsp;&nbsp;&nbsp;</span>
						<!--{/if}-->
					<!--{/loop}-->
				</li>
				<li class="clearfix"></li>
				<!--{/if}-->

				<!--{if !empty($_product['length_class']['w']) || !empty($_product['length_class']['h']) || !empty($_product['length_class']['l'])}-->
					<li class="key">{lang label_size}</li>
					<li class="value">
						<!--{if !empty($_product['length_class']['w'])}-->
							R{$_product['length_class']['w']}
						<!--{/if}-->
						<!--{if !empty($_product['length_class']['l'])}-->
							x D{$_product['length_class']['l']}
						<!--{/if}-->
						<!--{if !empty($_product['length_class']['h'])}-->
							x C{$_product['length_class']['h']} ($_product['length_class']['length_class']['name'])
						<!--{/if}-->



					</li>
					<li class="clearfix"></li>
				<!--{/if}-->


				<!--{if !empty($_product['weigh_class']['w'])}-->
