

<div class="f-product-view-info row">
	<div class="f-product-view-info-image col-md-5">
		<style>
			#f-pr-image-zoom-gallery .active img{
				opacity: 1 !important;
			}
		</style>
		<div class="f-pr-image-zoom">
			<div   class="zoomWrapper">
				<img id="f-pr-image-zoom-id" src="<!--{loadImage $_product['image'] resize 500 500}-->" data-zoom-image="<!--{loadImage $_product['image'] none null null}-->" class="img-responsive BNC-image-add-cart-{$_product['id']}" alt="{$_product['name']}">
			</div>
		</div>
		 <div class="f-pr-image-zoom-gallery">
			<div id="slidezoompage" >

				<div id="f-pr-image-zoom-gallery" class="owl-carousel">

				<!--{loop $_photo $k $v}-->

							<div class="pr-carol-item">
								<a href="javascript:;"   data-image="<!--{loadImage $v['image'] resize 500 500}-->" data-zoom-image="{BNCGLOBAL['upload_path']}{$v['image']}" title="{$v['att_title']}">
									<img src="<!--{loadImage $v['image'] crop 62 62}-->"  alt="{$v['att_alt']}"   />
								</a>
							</div>

				<!--{/loop}-->

				</div>

				 <script type="text/javascript">
				 	$(document).ready(function() {
 					//
	 					$("#f-pr-image-zoom-id").elevateZoom(
	 						{
	 							gallery:'f-pr-image-zoom-gallery',
	 							cursor: 'pointer',
	 							galleryActiveClass: 'active',
	 							imageCrossfade: true,
	 							loadingIcon: 'http://www.elevateweb.co.uk/spinner.gif',
	 							scrollZoom : true,
	 							easing : true
	 						});
	  					//
	  					$("#f-pr-image-zoom-id").bind("click", function(e) {
	  						var ez = $('#f-pr-image-zoom-id').data('elevateZoom');
	  						$.fancybox(ez.getGalleryList());
	  						return false;
	  					});
	  					//--------------
	  					$("#f-pr-image-zoom-gallery").owlCarousel();

					});
				 </script>
			</div>
			<div class="clearfix"></div>
		</div>
	</div>
	<div class="f-product-view-info-detail col-md-7">
		<div class="f-product-view-name">
			<h1>
				<span>{$_product['name']}</span>

			</h1>
			<br/>

			<div class="v2-rating">
				<div id="stars" data-text-login="{lang not_login_rate}" <!--{if !$_is_login}-->data-is-login="0" onclick="relogin()"<!--{/if}--> data-is-rater="<!--{if $_rate_user}-->1<!--{/if}-->" data-name="{$_product['name']}" data-id-product="{$_product['id']}" class="starrr" data-rating='{$_star_tb}' style="font-size: 12px;color: rgb(139, 139, 61);position: absolute;top: 5px;"></div>

				<div style="margin-left: 63px;margin-top: -12px;">
				<!--{if $_star_tb==0}-->
						<span id="display-first-rating" style="font-size: 12px">{lang first_rate}</span>

						<span id="display-total-rating" style="font-size: 12px">(<i class="fa fa-user"></i> <span id="rate" style="font-size: 12px;">{$_total_rate}</span>)</span>
					<!--{else}-->
						<span id="display-total-rating" style="font-size: 12px;">(<i class="fa fa-user"></i> <span id="rate" style="font-size: 12px;">{$_total_rate}</span>)</span>
					<!--{/if}-->
				</div>
			</div>


			<div class="f-pr-view-tool">
				<ul class="no-margin no-padding">
					<!--{if $_product['is_like']==1}-->
						<li><a href="javascript:;" class="like-product like-product-{$_product['id']}" data-is-info-page="1" data-like="{$_product['is_like']}" data-name="{$_product['name']}" data-id="{$_product['id']}" data-text-like="{lang like}" data-text-unlike="{lang unlike}" data-login="{lang not_login} <br/><a class='btn btn-success' href='{$web['home_url']}/user-login{$web['dotExtension']}'>{lang login}</a>"><i class="fa fa-thumbs-o-down"></i> {lang unlike}</a></li>
					<!--{elseif $_product['is_like']==0}-->
						<li><a href="javascript:;" class="like-product like-product-{$_product['id']}" data-is-info-page="1" data-like="{$_product['is_like']}" data-name="{$_product['name']}" data-id="{$_product['id']}" data-text-like="{lang like}" data-text-unlike="{lang unlike}" data-login="{lang not_login} <br/><a class='btn btn-success' href='{$web['home_url']}/user-login{$web['dotExtension']}'>{lang login}</a>"><i class="fa fa-thumbs-o-up"></i> {lang like}</a></li>
					<!--{else}-->
						<li><a href="javascript:;" class="like-product like-product-{$_product['id']}" data-is-info-page="1" data-like="{$_product['is_like']}" data-name="{$_product['name']}" data-id="{$_product['id']}" data-text-like="{lang like}" data-text-unlike="{lang unlike}" data-login="{lang not_login} <br/><a class='btn btn-success' href='{$web['home_url']}/user-login{$web['dotExtension']}'>{lang login}</a>"><i class="fa fa-thumbs-o-up"></i> {lang like}</a></li>
					<!--{/if}-->
					<li style="display:none"><a href="javascript:;">{lang label_compare}</a></li>
					<li>{lang label_view}: {$_product['view']}</li>
					<li>{lang label_date_create}: {$_product['time']}</li>
				</ul>
				<div class="clearfix"></div>
			</div>
		</div>
		<div class="f-pr-view-info">
