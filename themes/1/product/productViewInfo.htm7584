							<li class="value">
								{$v_other_info['content']}
							</li>
							<li class="clearfix"></li>
					<!--{/loop}-->
				<!--{/if}-->
				<!--{if $_product['status_size'] == 1 && $_product['quantity'] > 0}-->
				<!--{if !empty($_product['state'])}-->
					<li class="key">{lang condition}</li>
					<li class="value">

						<!--{loop $_product['state'] $k_state $v_state}-->
									<!--{if $v_state==1}-->
										{lang highlights}
									<!--{elseif $v_state==2}-->
										{lang comming}
									<!--{elseif $v_state==3}-->
										{lang ordering}
									<!--{elseif $v_state==4}-->
										{lang every_old}
									<!--{elseif $v_state==5}-->
										{lang hethang}
									<!--{/if}-->
						<!--{/loop}-->

					</li>
					<li class="clearfix"></li>
					<!--{/if}-->
				<!--{/if}-->


				<!--{if !empty($_product['brand'])}-->
					<li class="key">{lang brand}</li>
					<li class="value">
						<!--{loop $_product['brand'] $k_brand $v_brand}-->
									<a href="{$v_brand['link']}" class="brand">{$v_brand['name']}</a>
						<!--{/loop}-->
					</li>
					<li class="clearfix"></li>
				<!--{/if}-->

			</ul>
			<div class="clearfix"></div>
		</div>
		<!--{if $_product['status_size'] == 1 && $_product['quantity'] > 0}-->

			<!--{if !empty($_shop)}-->

			<div class="f-pr-view-choosesizeGroup padding-10">
