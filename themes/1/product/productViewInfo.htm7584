					<label>{lang label_shop_name}</label>
					<ul class="v2-dealer">

		            	<!--{loop $_shop $k $v}-->
		            		<!--{if !empty($v['quantity'])}-->
		        				<li>{lang product_ready} {$v['quantity']} sp - {lang phone}:{$v['phone']} - {$v['address']} - {$v['name']}</li>
		        			<!--{/if}-->
						<!--{/loop}-->
					</ul>

	            </div>
	            <!--{if $_shop_count > 3}-->
						<button class="btn" id="more-shop" style="margin:5px;">{lang view_more}</buton>
				<!--{/if}-->
	        </div>

			<div class="f-pr-view-box-size" style="height: 100px;">
				<span class="box-size-title">{lang select_size}</span>
				<!--{loop $_shop_v2 $k $v}-->
					<div data-name-size="{$v['name']}" <!--{if $k==0}-->style="border: 1px solid rgb(255, 0, 0);"<!--{/if}--> class="size">{$v['name']}</div>
				<!--{/loop}-->

				<div id="quantity">{lang label_quantity}
				<!--{loop $_shop_v2 $k $v}-->
					<select class="quantity" id="quantity-{$v['name']}" name="quantity" class="form-control" style="width: 100px;">
						<!--{loopfor $i=1 $i<=$v['quantity'] $i++}-->
					            <option value="{$i}">{$i}</option>
					    <!--{/loopfor}-->
					</select>
				<!--{/loop}-->
					<button class="add-cart button-small" data-size="{$v1['size_id']}" data-shop="1" data-product="{$_product['id']}">{lang label_add_to_cart}</button>
				</div>
			</div>
			<!--{else}-->
			 <label>{lang hethang}</label>
			 <!--{/if}-->
		<!--{else}-->
			<!--{if !empty($_shop) && $_product['quantity'] > 0}-->
			<div class="f-pr-view-choosesizeGroup padding-10">
				<div class="dealer-tab" data-is-more="0" id="shop-content" style="<!--{if $_shop_count > 3}-->overflow: hidden;height: 120px;<!--{/if}-->">
					<label>{lang label_shop_name}</label>
					<ul class="v2-dealer">
		            	<!--{loop $_shop $k $v}-->
		            		<!--{if !empty($v['quantity'])}-->
		        				<li>{lang product_ready} {$v['quantity']} sp - {lang phone}:{$v['phone']} - {$v['address']} - {$v['name']}</li>
		        			<!--{/if}-->
						<!--{/loop}-->
					</ul>

	            </div>
	            <!--{if $_shop_count > 3}-->
						<button class="btn" id="more-shop" style="margin:5px;">{lang view_more}</buton>
				<!--{/if}-->
	        </div>

	        <!--{/if}-->
	        <!--{if $_total_item_shop > 0}-->
	        <div class="f-pr-view-box-size">
				<span class="box-size-title">{lang label_quantity}</span>
				<div class="select-shop" id="shop-1">
					<!--{if $_total_item_shop > 0}-->
                	<select>
                		<!--{loopfor $i=1 $i<=$_total_item_shop $i++}-->
                			<option value="{$i}">{$i}</option>
                		<!--{/loopfor}-->
                	</select>
                	<!--{else}-->
                	<span>{lang no_sold}</span>
                	<!--{/if}-->
				</div>
			</div>
			<!--{/if}-->
		<!--{/if}-->
		<!--{if !empty($_group_color)}-->
		<div class="f-pr-view-chooseColorGroup padding-10">
			<span class="box-size-title">{lang color}</span>
		    <div class="clearfix"></div>
		    <div class="size-option">
		    	<ul>
		        	<!--{loop $_group_color $k $v}-->
		        	<li <!--{if $k==0}-->class="active"<!--{/if}-->>
		            	<div class="row">
		                	<div class="col-lg-2"><img src="<!--{loadImage $v['image'] resize 49 49}-->" class="img-responsive"></div>
		                    <div class="col-lg-5"><a class="namecolor" href="{$v['link']}">{$v['name']}</a></div>
		                    <div class="col-lg-3">
		                    	<!--{if $v['price_old'] != 0}--><span class="csg-oldprice">{$v['price_old']}</span><!--{/if}-->
		                    	<span class="csg-price">{$v['price']}</span>
		                    </div>
		                    <!--<div class="col-lg-1">
		                    	<!--{if $v['quantity'] > 0}-->
		                    	<select>
		                    		<!--{loopfor $i=1 $i<=$v['quantity'] $i++}-->
		                    		<option value="{$i}">{$i}</option>
		                    		<!--{/loopfor}-->
		                    	</select>
		                    	<!--{else}-->
		                    	<span>{lang no_sold}</span>
		                    	<!--{/if}-->
		                    </div>-->
		                </div>
		            </li>
		            <!--{/loop}-->
		        </ul>
		    </div>
		</div>
		<!--{/if}-->
        <!--{if $_product['quantity'] > 0 && $_product['show_cart'] == 1 && $_product['status_size'] != 1}-->
        	<!--{if empty($_shop)}-->
			<a href="javascript:;" class="button-small" data-toggle="modal" data-target="#quickBuy">
				<span class="glyphicon glyphicon-shopping-cart"></span> {lang buy_now}
			</a>
			<!--{/if}-->
			<a href="javascript:;" class="button-small" id="add-cart" data-product="{$_product['id']}">
				<i class="fa fa-shopping-cart"></i> {lang label_add_to_cart}
			</a>
		<!--{elseif $_product['quantity'] == 0}-->
			<label>{lang no_sold}</label>

		<!--{/if}-->
        <div class="clearfix"></div>
